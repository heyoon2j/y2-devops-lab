{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BucketPolicy",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::585796627604:role/dms-access-for-endpoint"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:GetObjectVersion",
                "s3:GetBucketPolicy",
                "s3:PutBucketPolicy",
                "s3:DeleteBucketPolicy",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:DeleteBucket"
            ],
            "Resource": [
                "arn:aws:s3:::dms-6w2vchjfdsnm23im75e4342xt73qcc5fwhxjzbq/*",
                "arn:aws:s3:::dms-6w2vchjfdsnm23im75e4342xt73qcc5fwhxjzbq"
            ]
        },
        {
            "Sid": "DenyUnEncryptedObjectUploads",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::dms-6w2vchjfdsnm23im75e4342xt73qcc5fwhxjzbq/*",
            "Condition": {
                "ForAllValues:StringNotEquals": {
                    "s3:x-amz-server-side-encryption": [
                        "AES256",
                        "aws:kms"
                    ]
                }
            }
        }
    ]
}

